   "source": [
    "USE CATALOG workspace;\n",
    "USE SCHEMA salesdb;\n",
    "SHOW TABLES;\n",
    "SELECT\n",
    "  p.category,\n",
    "  SUM(f.sales_amount) AS sales\n",
    "FROM fact_sales f\n",
    "JOIN dim_products p\n",
    "  ON f.product_key = p.product_id\n",
    "GROUP BY p.category\n",
    "ORDER BY sales DESC;\n"
   ]
  }
 ],
